{
  "name": "ejs-layout-string",
  "type": "module",
  "version": "1.0.2",
  "packageManager": "pnpm@9.14.3",
  "description": "EJS layout renderer that returns HTML strings with custom layout path support",
  "author": {
    "name": "Aakash Srivastav",
    "email": "akhsri21@gmail.com",
    "url": "https://github.com/akhsri"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/akhsri",
  "homepage": "https://github.com/redbus-labs/ejs-layout-string#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redbus-labs/ejs-layout-string.git"
  },
  "bugs": "https://github.com/redbus-labs/ejs-layout-string/issues",
  "keywords": [
    "ejs",
    "layout",
    "template",
    "render",
    "rendering",
    "html",
    "express",
    "ejs string",
    "ejs layout",
    "html caching",
    "server side rendering",
    "ssr"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish --registry=https://registry.npmjs.org --access public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "ejs": "3.1.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.10.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.15.4",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
